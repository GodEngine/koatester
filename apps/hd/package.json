    "dev": "cross-env node ./npm-scripts/dev.js",
    "start": "cross-env  NODE_ENV=development webpack --hide-modules",
    "build": "cross-env node ./npm-scripts/build.js",
    "lint": "eslint ./src ./webpack.config.js -f table || true",
    "clean:coverage": "rimraf ./coverage",
    "test:unit": "yarn run clean:coverage && karma start --single-run --browsers PhantomJS",
    "test:unit:watch": "yarn run clean:coverage && karma start --auto-watch --single-run --browsers PhantomJS",
    "test:jest": "NODE_ENV=test node ./test/unit/entry.js",
    "coverage": "http-server -p 1234 ./coverage",
    "test:e2e:install": "selenium-standalone install",
    "test:e2e:svr": "selenium-standalone start",
    "test:e2e": "nightwatch",
    "sprite": "webpack --config ./components/webpack.config.babel.js --progress --hide-modules"
